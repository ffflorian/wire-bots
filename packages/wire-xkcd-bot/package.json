{
  "author": "Florian Keller <github@floriankeller.de>",
  "dependencies": {
    "@ffflorian/xkcdjs": "1.3.4",
    "@wireapp/bot-api": "1.0.1",
    "dotenv": "6.0.0",
    "jpeg-js": "0.3.4",
    "logdown": "3.2.7",
    "moment": "2.22.2",
    "omggif": "1.0.9",
    "pm2": "3.1.2",
    "png.js": "0.2.1"
  },
  "devDependencies": {
    "@ffflorian/jszip-cli": "1.4.2",
    "@types/jpeg-js": "0.3.0",
    "@types/node": "10.10.0",
    "@types/omggif": "1.0.0",
    "@types/png.js": "0.2.0",
    "@types/request": "2.47.1",
    "cross-env": "5.2.0",
    "prettier": "1.14.2",
    "rimraf": "2.6.2",
    "typescript": "3.0.3"
  },
  "license": "GPL-3.0",
  "main": "dist/index.js",
  "name": "@ffflorian/wire-xkcd-bot",
  "private": true,
  "repository": "https://github.com/ffflorian/wire-bots/tree/master/packages/wire-xkcd-bot",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "deploy": "yarn dist && yarn zip && eb deploy -nh",
    "dist": "yarn clean && yarn build",
    "lint": "tslint -p tsconfig.json --fix",
    "prettier": "prettier --single-quote --write src/**/*.ts",
    "start": "pm2 start && pm2 logs",
    "start:dev": "cross-env NODE_DEBUG=\"@wireapp/*,wire-xkcd-bot/*\" node dist/index.js",
    "test": "yarn dist && exit 0",
    "zip": "jszip-cli add dist .ebextensions package.json ecosystem.config.js -q -o dist/wire-xkcd-bot.zip"
  },
  "version": "1.0.0"
}
